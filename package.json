{
  "name": "data-by-design",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint . && cd client && vue-cli-service lint",
    "generate:env": "node scripts/env-gen",
    "test:client": "cd client && vue-cli-service test:unit",
    "test:api": "mocha server-express/test",
    "posttest": "npm run lint && nsp check",
    "build": "cd client && vue-cli-service build",
    "api:dev": "NODE_ENV=development nodemon server-express/app.js",
    "app:dev": "cd client && vue-cli-service serve",
    "start:dev": "run-p api:dev app:dev",
    "api": "NODE_ENV=production forever start server-express/app.js",
    "start": "run-s build api",
    "start:reset": "RESET_DB=true npm run start",
    "restart": "run-s stop start",
    "stop": "forever stop server-express/app.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/vue-fontawesome": "^0.1.2",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "clone-deep": "^4.0.0",
    "compression": "^1.0.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.5.2",
    "csurf": "^1.9.0",
    "csv-parse": "^2.5.0",
    "d3": "^5.9.1",
    "dotenv": "^6.2.0",
    "dxd-common": "file:common",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.3.1",
    "express-session": "^1.17.0",
    "helmet": "^3.10.0",
    "http-proxy-middleware": "^0.19.1",
    "i": "^0.3.6",
    "js-event-bus": "^1.0.0",
    "jwks-rsa": "^1.6.0",
    "loopback": "^3.25.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.3.1",
    "loopback-connector-mysql": "^2.4.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.3",
    "morgan": "^1.9.1",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "register-service-worker": "^1.0.0",
    "serve-favicon": "^2.0.1",
    "stickybits": "^3.4.1",
    "stream-transform": "^1.0.2",
    "strong-error-handler": "^3.0.0",
    "vue": "^2.6.11",
    "vue-notification": "^1.3.12",
    "vue-router": "^3.0.1",
    "vue-scroll": "^2.1.7",
    "vue-slicksort": "^1.1.3",
    "vuex": "^3.0.1",
    "waypoints": "^4.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.0.0-rc.5",
    "@vue/cli-plugin-unit-mocha": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "chai": "^4.1.2",
    "chai-http": "^4.3.0",
    "errorhandler": "^1.5.1",
    "eslint": "^4.12.1",
    "eslint-config-loopback": "^8.0.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "nsp": "^2.1.0",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "2500"
  },
  "license": "UNLICENSED",
  "description": "vueback"
}
