{
  "name": "data-by-design",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint . && cd client && vue-cli-service lint",
    "test:unit": "cd client && vue-cli-service test:unit",
    "posttest": "npm run lint && nsp check",
    "build": "cd client && vue-cli-service build",
    "api:dev": "NODE_ENV=development nodemon .",
    "app:dev": "cd client && vue-cli-service serve",
    "start:dev": "run-p api:dev app:dev",
    "api": "NODE_ENV=production forever start server/server.js",
    "start": "run-s build api",
    "start:reset": "RESET_DB=true npm run start",
    "restart": "run-s stop start",
    "stop": "forever stop server/server.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "clone-deep": "^4.0.0",
    "compression": "^1.0.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.5.2",
    "csurf": "^1.9.0",
    "csv-parse": "^2.5.0",
    "d3": "^5.5.0",
    "dotenv": "^6.0.0",
    "express-history-api-fallback": "^2.2.1",
    "helmet": "^3.10.0",
    "loopback": "^3.19.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.0.0",
    "loopback-connector-mysql": "^2.4.1",
    "register-service-worker": "^1.0.0",
    "serve-favicon": "^2.0.1",
    "stickybits": "^3.4.1",
    "stream-transform": "^1.0.2",
    "strong-error-handler": "^3.0.0",
    "vue": "^2.5.16",
    "vue-notification": "^1.3.12",
    "vue-router": "^3.0.1",
    "vue-scroll": "^2.1.7",
    "vuex": "^3.0.1",
    "waypoints": "^4.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "nsp": "^2.1.0",
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.5",
    "@vue/cli-plugin-pwa": "^3.0.0-rc.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "vue-template-compiler": "^2.5.16"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "2500"
  },
  "license": "UNLICENSED",
  "description": "vueback"
}
